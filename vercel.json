{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/admin(/.*)?",
      "dest": "/admin/index.html"
    },
    {
      "src": "/((?!admin|api|sw\\.js|manifest\\.json|icon-.*|.*\\.).*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*).(jpg|jpeg|png|gif|webp|svg|ico)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}