{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin/index.html"
    },
    {
      "source": "/((?!admin|api|sw\\.js|manifest\\.json|icon-.*|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}